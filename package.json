{
  "name": "vscode-modal-keys",
  "displayName": "ModalKeys",
  "description": "Customizable modal key bindings (ala vim)",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "modalkeys.importBindings",
        "title": "ModalKeys: Insert Keybinding Configuration"
      }
    ]
  },
  "watch": {
    "json-specs": {
      "extensions": "ts"
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "precompile": "npm run binding-json-spec",
    "compile": "webpack",
    "watch-specs": "npm-watch json-specs",
    "watch": "webpack --watch",
    "prepackage": "npm run json-spec",
    "package": "webpack --mode production --devtool hidden-source-map",
    "json-specs": "npx typescript-json-schema tsconfig.json IBindingSpec -o json_specs/binding_specs.json",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run binding-json-spec && npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/object-hash": "^3.0.6",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "npm-watch": "^0.11.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "typescript-json-schema": "^0.62.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "ajv": "^6.12.6",
    "js-toml": "^1.0.0",
    "lodash": "^4.17.21",
    "object-hash": "^3.0.0",
    "semver": "^7.5.4"
  }
}
